<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.ars.automation"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />

    <application android:icon="@drawable/home" android:label="@string/app_name">
        <activity android:name=".ARSAutomationActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



<activity android:name=".menu"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.ars.automation.MENU" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


   
   
   <activity android:name=".About"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.ars.automation.ABOUT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
   
   
   
   
   
   
   
   
   
    </application>
</manifest>